package mavendemo;

public class HelloBean {
	public String name;
	public int age;
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	public void sayHello() {
		System.out.println("Hello "+this.name+" your Age is "+this.age);
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
}
